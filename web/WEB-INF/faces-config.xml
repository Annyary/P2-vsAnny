<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ACCESO</from-outcome>
            <to-view-id>/bienvenida.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ERROR</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>REGISTRARSE</from-outcome>
            <to-view-id>/registro.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>REGISTRADO</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/bienvenida.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>SALIRSISTEMA</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/perfil.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/recuperarContrasenna.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ENVIADO</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
